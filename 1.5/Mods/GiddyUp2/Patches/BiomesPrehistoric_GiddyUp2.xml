<Patch>

	<!-- Ankylosauria grant a small amount of extra protection to their rider. -->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/PawnKindDef[defName="BMT_Ankylosaurus" or defName="BMT_Borealopelta" or
			defName="BMT_ChromaticAnkylosaurus" or defName="BMT_Minmi" or defName="BMT_Polacanthus"]
		</xpath>
		<value>
			<li Class="GiddyUp.CustomStats">
				<armorModifier>1.1</armorModifier>
			</li>
		</value>
	</Operation>
	<!-- Ceratopsia grant extra protection to their rider. -->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/PawnKindDef[defName="BMT_Centrosaurus" or defName="BMT_Chasmosaurus" or
			defName="BMT_Styracosaurus" or defName="BMT_Triceratops"]
		</xpath>
		<value>
			<li Class="GiddyUp.CustomStats">
				<armorModifier>1.25</armorModifier>
			</li>
		</value>
	</Operation>
	<!-- Ornithomimosauria are faster when mounted. -->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/PawnKindDef[defName="BMT_Gallimimus" or defName="BMT_Ornithomimus" or
			defName="BMT_Struthiomimus"]
		</xpath>
		<value>
			<li Class="GiddyUp.CustomStats">
				<speedModifier>1.4</speedModifier>
			</li>
		</value>
	</Operation>
