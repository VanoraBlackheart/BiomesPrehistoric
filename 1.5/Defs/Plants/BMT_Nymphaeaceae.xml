<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BushBase">
    <defName>BMT_Nymphaeaceae</defName>
    <label>water lily</label>
    <description>Aquatic plants with disk shaped leaves and huge, beautiful flowers. Unable to support the weight of humans or dinosaurs.</description>
    <graphicData>
      <texPath>BMT_Prehistoric/Plant/Nymphaeaceae</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <drawSize>1.7</drawSize>
    </graphicData>
    <statBases>
      <Beauty>5</Beauty>
      <BeautyOutdoors>5</BeautyOutdoors>
      <Nutrition>1.5</Nutrition>
    </statBases>
    <plant Inherit="False">
      <growDays>10</growDays>
      <wildClusterRadius>5</wildClusterRadius>
      <wildClusterWeight>200</wildClusterWeight>
      <lifespanDaysPerGrowDays>6</lifespanDaysPerGrowDays>
      <fertilityMin>0.1</fertilityMin>
      <fertilitySensitivity>0.3</fertilitySensitivity>
      <topWindExposure>0.40</topWindExposure>
      <wildOrder>2</wildOrder>
      <interferesWithRoof>false</interferesWithRoof>
    </plant>
    <ingestible />
        <modExtensions>
            <li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
                <terrainTags>
                    <li>Water</li>
                    <li>Fresh</li>
                    <li>Shallow</li>
                    <li>Boggy</li>
                </terrainTags>
            </li>
        </modExtensions>
  </ThingDef>

</Defs>