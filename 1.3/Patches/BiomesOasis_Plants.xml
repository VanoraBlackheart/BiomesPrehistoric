<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biomes! Oasis</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> 
					<xpath>Defs/BiomeDef[defName="BMT_ChromaticOasis"]/wildPlants</xpath>
					<value>
			<BMT_Calamites>0.1</BMT_Calamites>
			<BMT_Clubmoss>0.5</BMT_Clubmoss>
					</value>
				</li>
			</operations>
		</match>
	</Operation>


</Patch>

